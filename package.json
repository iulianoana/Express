{
  "name": "jsdev",
  "version": "1.0.0",
  "description": "This is a test project for setting up a Javascript Development Environment",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "preopen:src": "babel-node buildScripts/startMessage.js",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "npm audit",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch-extensions ts",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "start-mockapi": "json-server --watch src/api/db.json --port 30001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulianoana/Express.git"
  },
  "keywords": [
    "javascript",
    "development",
    "environment"
  ],
  "author": "Iulian Oana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulianoana/Express/issues"
  },
  "homepage": "https://github.com/iulianoana/JSDev",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^9.8.0",
    "json-schema-faker": "^0.5.0-rc9",
    "json-server": "^0.14.2",
    "localtunnel": "^1.9.1",
    "mocha": "^6.0.0",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "surge": "^0.20.1",
    "webpack": "^4.29.5",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-md5-hash": "0.0.6",
    "whatwg-fetch": "^3.0.0"
  }
}
